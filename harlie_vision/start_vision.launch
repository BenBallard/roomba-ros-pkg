<launch>
	<node machine="mainCPU" name="front_camera" pkg="camera1394" type="camera1394" respawn="true" >
		<param name="guid" type="string" value="00b09d010070c880"/>
		<param name="video_mode" value="640x480_mono" />
		<param name="fps" value="30" />
		<param name="whitebalance" value="100" />
		<param name="shutter" value="150" />
		<param name="bayer_pattern" value="GBRG" />
		<param name="bayer_method" value="Simple" />
		<param name="camera_info_manager/url" value="file:///opt/igvc_ros/harlie_vision/calibration/calibration_front_camera.yaml" />
		<param name="frame_id" value="front_camera" />
	</node>
	<node machine="mainCPU" pkg="image_proc" type="image_proc" name="proc" ns="front_camera/camera" />
	<node machine="mainCPU" pkg="tf" type="static_transform_publisher" name="front_camera_tf_broadcaster" args="0.2286 0.0635 1.676 -0.0873 0.608246 0 base_link front_camera 100" />
</launch>
